{
  "name": "azdevops-kube-summary",
  "version": "1.0.12",
  "description": "Azure DevOps Kubernetes Summary",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/azpipelines-kubernetesUI.git"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "compile": "npm run clean && tsc --skipLibCheck && npm run copy-all",
    "compile:dev": "npm run clean && webpack --mode development",
    "build": "npm run compile",
    "build:dev": "npm run compile:dev && npm run postbuild",
    "postbuild": "npm run package-extension -- --rev-version",
    "compile-vsix": "npm run clean",
    "bvsix": "npm run compile-vsix && webpack --mode development",
    "postbvsix": "npm run package-extension -- --rev-version",
    "package-extension": "tfx extension create --manifest-globs azure-devops-extension.json src/WebUI/**/*.json",
    "publish-extension": "tfx extension publish --manifest-globs azure-devops-extension.json src/WebUI/**/*.json",
    "copy-all": "npm run copy-html && npm run copy-resources",
    "copy-html": "copyfiles -u 1 src/**/*.html src/**/*.scss dist",
    "copy-resources": "copyfiles -u 1 src/**/Resources.js src/**/Types.d.ts dist"
  },
  "keywords": [
    "extensions",
    "Azure DevOps",
    "Visual Studio Team Services"
  ],
  "license": "MIT",
  "main": "dist/WebUI/Components/KubeSummary.js",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@kubernetes/client-node": "^0.7.1",
    "azure-devops-extension-api": "^1.143.0",
    "azure-devops-extension-sdk": "~2.0.3",
    "azure-devops-ui": "~1.143.0",
    "office-ui-fabric-react": "5.131.5",
    "react": "~16.3.2",
    "react-dom": "~16.3.2"
  },
  "devDependencies": {
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "base64-inline-loader": "^1.1.1",
    "copy-webpack-plugin": "^4.6.0",
    "copyfiles": "^2.1.0",
    "css-loader": "^1.0.1",
    "file-loader": "~2.0.0",
    "node-sass": "~4.9.4",
    "recursive-copy": "~2.0.7",
    "rimraf": "~2.6.2",
    "sass-loader": "~7.1.0",
    "style-loader": "~0.23.1",
    "tfx-cli": "^0.6.3",
    "ts-loader": "~5.2.2",
    "typescript": "^2.9.2",
    "uglify-es": "~3.1.3",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2"
  }
}
