{
  "name": "@azurepipelines/azdevops-kube-summary",
  "version": "1.0.6",
  "description": "Azure DevOps Kubernetes Summary",
  "engines": {
    "node": ">=8.4.0",
    "npm": ">=5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/azpipelines-kubernetesUI.git"
  },
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./dist_tests",
    "compile": "npm run clean && npm run compilesrc && npm run compiletest && npm run copy-all",
    "build": "npm run compile",
    "copy-all": "npm run copy-scss && npm run copy-resources",
    "copy-scss": "copyfiles -u 1 src/**/*.scss dist",
    "copy-resources": "copyfiles -u 1 src/**/Resources.js src/**/Types.d.ts dist",
    "test": "npm run compiletest && jest ./dist_tests",
    "compiletest": "tsc -p ./tests --outDir ./dist_tests",
    "compilesrc": "tsc -p ./src --outDir dist"
  },
  "keywords": [
    "Kubernetes-UI"
  ],
  "license": "MIT",
  "main": "dist/WebUI/Components/KubeSummary.js",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@kubernetes/client-node": "~0.7.1",
    "azure-devops-ui": "~1.144.1",
    "office-ui-fabric-react": "5.131.5",
    "react": "~16.3.2",
    "react-dom": "~16.3.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "base64-inline-loader": "^1.1.1",
    "copy-webpack-plugin": "^4.6.0",
    "copyfiles": "^2.1.0",
    "css-loader": "^1.0.1",
    "file-loader": "~2.0.0",
    "jest": "^23.6.0",
    "node-sass": "~4.9.4",
    "recursive-copy": "~2.0.7",
    "rimraf": "~2.6.2",
    "sass-loader": "~7.1.0",
    "style-loader": "~0.23.1",
    "ts-loader": "~5.2.2",
    "typescript": "^2.9.2",
    "webpack": "^4.27.0"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/azpipelines-kubernetesUI/issues"
  },
  "homepage": "https://github.com/Microsoft/azpipelines-kubernetesUI#readme",
  "author": "Azure Pipelines"
}
